{
	"name": "Copy by Ivan",
	"version": "0.2",
	"manifest_version": 2,
	"description": "Kopira podatke iz Jaspera.",
	"icons": {
		"48": "/asset/images/copy.png"
	},
	"permissions": ["tabs", "activeTab", "declarativeContent", "storage"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"page_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"48": "/asset/images/copy.png"
		}
	},
	"commands": {
		"_execute_browser_action": {
			"suggested_key": {
				"default": "Ctrl+Shift+F",
				"mac": "MacCtrl+Shift+F"
			}
		}
	}
}
